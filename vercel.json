{
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
        {
            "source": "/data/(.*)",
            "headers": [
                { "key": "Cache-Control", "value": "public, max-age=3600" }
            ]
        },
        {
            "source": "/(.*).js",
            "headers": [
                { "key": "Cache-Control", "value": "public, max-age=86400" }
            ]
        },
        {
            "source": "/(.*).css",
            "headers": [
                { "key": "Cache-Control", "value": "public, max-age=86400" }
            ]
        }
    ]
}
